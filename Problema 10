using System;

using System.Linq;

using System.Collections;

using System.Collections.Generic;

namespace Ejercicio10

{

    class program

    {
        static void Main(string[] args)

        {
            string contrasena;

            Console.WriteLine("Digite la contrase単a");

            contrasena = (System.Console.ReadLine());


            while (contrasena != "iloveyou123")

            {
                if (contrasena != "iloveyou123")
                {
                    Console.WriteLine("Contrase単a incorrecta");
                    Console.WriteLine("Escribir contrase単a de nuevo");
                    contrasena = (System.Console.ReadLine());
                }
               
                    if (contrasena == "iloveyou123")
                        Console.WriteLine("Contrase単a correcta");
                
            }
        }
    }
}
